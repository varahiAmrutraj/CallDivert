
@{
    ViewBag.Title = "Create";
}
<div class="content-wrapper" style="min-height: 946.281px;">
    <section class="content-header">
        <h1>
            Create Schedule
        </h1>
    </section>

    <section class="content">
        <div class="row">
            <!-- left column -->
            <div class="col-md-12">
                <!-- general form elements -->
                <div class="box box-primary">
                    <!-- /.box-header -->
                    <!-- form start -->
                    <form role="form">
                        <div class="box-body">
                            <div class="form-group">
                                <label for="exampleInputUser">User</label>
                                <select class="form-control" id="exampleInputUser">
                                    <option>User 1</option>
                                    <option>User 2</option>
                                    <option>User 3</option>
                                    <option>User 4</option>
                                    <option>User 5</option>
                                </select>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputDay">Select Day</label>
                                <select class="form-control" id="exampleInputDay">
                                    <option>Monday</option>
                                    <option>Tuesday</option>
                                    <option>Wednesday</option>
                                    <option>Thursday</option>
                                    <option>Friday</option>
                                </select>
                            </div>
                            <div class="form-group">
                                <label for="exampleInputStartTime">Start Time</label>
                                <input type="time" class="form-control" id="exampleInputStartTime">
                            </div>
                            <div class="form-group">
                                <label for="exampleInputEndTime">End Time</label>
                                <input type="time" class="form-control" id="exampleInputEndTime">
                            </div>
                        </div>
                        <!-- /.box-body -->
                        <div class="box-footer">
                            <button type="submit" class="btn btn-primary">Submit</button>
                        </div>
                    </form>
                </div>
                <!-- /.box -->
             </div>
            <!--/.col (left) -->
        </div>
        <!-- /.row -->
    </section>
</div>

